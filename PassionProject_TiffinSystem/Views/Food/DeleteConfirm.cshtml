@model PassionProject_TiffinSystem.Models.FoodDto
@{
    ViewBag.Title = "DeleteConfirm";
}
<div style=" border: 1px solid #000; padding: 1em; margin: 1em;">
    <h2>Are you sure you want to delete this food Item?</h2>
    <a href="/Food/Detail">Cancel</a>
    <p>@Model.ItemName</p>
    <form action="/Food/Delete/@Model.FoodID" method="POST">
        <input style="color:#ab2020;" type="submit" value="Confirm Delete" />

    </form>
</div>