@model IEnumerable<PassionProject_TiffinSystem.Models.FoodDto>
@{
    ViewBag.Title = "List";
}
<p><a href="/Food/New">New Food Item</a></p>
<h2>List of Food Items</h2>
<div style=" border: 1px solid #000; padding: 1em; margin-top: 1em;">
    @foreach (var food in Model)
    {
        <a href="/Food/Detail/@food.FoodID"><p>@food.ItemName</p></a>
        <hr />
    }

</div>