@model PassionProject_TiffinSystem.Models.SchedulexMealDto
@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>DeleteConfirm</h2>


<div style=" border: 1px solid #000; padding: 1em; margin-top: 1em;">
    <h2>Are you sure you want to delete this Menu?</h2>
    <a href="/SchedulexMeal/List">Cancel</a>
    <p>StartDate: @Model.StartDate To EndDate: @Model.EndDate</p>
    <p>Day: @Model.Day</p>
    <p>MealType: @Model.Mealtype</p>
    <form action="/SchedulexMeal/Delete/@Model.SchedulexMealId" method="POST">
        <input style="color:#ab2020;" type="submit" value="Confirm Delete" />

    </form>
</div>